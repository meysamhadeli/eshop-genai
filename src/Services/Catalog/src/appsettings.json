{
  "AppOptions": {
    "Name": "Catalog-Service",
    "UiUrl": "http://localhost:5173"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "PostgresOptions": {
    "ConnectionString": "Server=localhost;Port=5432;Database=catalog;User Id=postgres;Password=postgres;Include Error Detail=true"
  },
  "PersistMessageOptions": {
    "Interval": 30,
    "Enabled": true,
    "ConnectionString": "Server=localhost;Port=5432;Database=persist_message;User Id=postgres;Password=postgres;Include Error Detail=true"
  },
  "HybridCacheOptions": {
    "RedisConnectionString": "localhost:6379",
    "RedisExpireMinutes": 60,
    "InMemoryExpireMinutes": 30,
    "UseDistributedLock": true,
    "InstanceName": "Catalog:"
  },
  "RabbitMqOptions": {
    "HostName": "localhost",
    "ExchangeName": "catalog",
    "UserName": "guest",
    "Password": "guest",
    "Port": 5672
  },
  "HealthOptions": {
    "Enabled": false
  },
  "SemanticSearchOptions": {
    "Enabled": true,
    "Provider": "Ollama",
    "EmbeddingModel": "nomic-embed-text",
    "BaseUrl": "http://localhost:11434",
    "VectorDbConnectionString": "http://localhost:6334",
    "SimilarityThreshold": 0.5,
    "MaxResults": 10              
  },
  "ObservabilityOptions": {
    "InstrumentationName": "catalog_service",
    "OTLPOptions": {
      "OTLPGrpExporterEndpoint": "http://localhost:4317"
    },
    "AspireDashboardOTLPOptions": {
      "OTLPGrpExporterEndpoint": "http://localhost:4319"
    },
    "ZipkinOptions": {
      "HttpExporterEndpoint": "http://localhost:9411/api/v2/spans"
    },
    "JaegerOptions": {
      "OTLPGrpcExporterEndpoint": "http://localhost:14317",
      "HttpExporterEndpoint": "http://localhost:14268/api/traces"
    },
    "UsePrometheusExporter": true,
    "UseOTLPExporter": true,
    "UseAspireOTLPExporter": true,
    "UseGrafanaExporter": false,
    "ServiceName": "Catalog Service"
  },
  "Jwt": {
    "Authority": "http://localhost:6005",
    "Audience": "catalog-api"
  },
  "AllowedHosts": "*"
}
